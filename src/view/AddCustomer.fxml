      <TextField fx:id="nameField" layoutX="45.0" layoutY="127.0" promptText="Name" />
      <TextField fx:id="addressField" layoutX="45.0" layoutY="170.0" promptText="Address" />
      <TextField fx:id="postalCodeField" layoutX="45.0" layoutY="214.0" promptText="Postal Code" />
      <TextField fx:id="phoneField" layoutX="45.0" layoutY="260.0" promptText="Phone" />
      <ComboBox fx:id="countrySelection" layoutX="225.0" layoutY="113.0" prefWidth="150.0" />
      <ComboBox fx:id="firstLevelDivisionSelection" layoutX="225.0" layoutY="194.0" prefWidth="150.0" />
      <Label layoutX="225.0" layoutY="86.0" text="Country" />
      <Label layoutX="225.0" layoutY="161.0" text="First-Level Division" />
      <TextField fx:id="idField" editable="false" layoutX="45.0" layoutY="86.0" />
   </children>
</AnchorPane>
